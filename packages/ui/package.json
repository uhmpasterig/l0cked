{
  "name": "@locked/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./src/styles.css",
    "./components/*": "./src/components/*.tsx",
    "./shadcn/*": "./src/components/ui/*.tsx",
    "./navigation/*": "./src/components/navigation/*.tsx"
  },
  "scripts": {
    "ui:add": "pnpm dlx shadcn-ui@latest add",
    "build": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.323.0",
    "react": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@locked/eslint-config": "workspace:*",
    "@locked/tailwind-config": "workspace:*",
    "@locked/typescript-config": "workspace:*",
    "@turbo/gen": "^1.11.3",
    "@types/eslint": "^8.56.1",
    "@types/locked": "workspace:*",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "zustand": "^4.5.0"
  }
}
